  "scripts": {
    "start": "npm run start:prod",
    "start:dev": "webpack-dev-server",
    "start:prod": "cross-env NODE_ENV=production NODE_PORT=8081 node server.js",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production npm run clean && webpack --colors --profile --config webpack.config.prod.js",
    "build:dev": "cross-env NODE_ENV=development npm run clean && webpack --colors --profile --config webpack.config.js",
    "lint": "eslint --fix src",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "clean": "shx rm -rf dist",
    "pristine": "npm run clean && shx rm -rf node_modules",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
