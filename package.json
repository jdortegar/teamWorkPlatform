    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "classnames": "^2.2.6",
    "connected-react-router": "6.3.1",
    "copy-webpack-plugin": "^5.0.3",
    "countries-and-timezones": "^1.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "emoji-mart": "^2.11.0",
    "express": "^4.16.3",
    "file-loader": "^3.0.1",
    "helmet": "^3.16.0",
    "highcharts": "^7.1.1",
