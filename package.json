{
  "name": "webapp",
  "description": "The Habla UI",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Habla-Inc/webapp.git"
  },
  "bugs": {
    "url": "https://github.com/Habla-Inc/webapp/issues"
  },
  "homepage": "https://habla.ai",
  "author": "Tho Truong <tho.truong@habla.ai>, Anthony Daga <anthony.daga@habla.ai>, Mike Somlo <mike.somlo@habla.ai>, Gonzalo Marquez <gonzalo@techlabs.com>",
  "license": "HABLA",
  "engines": {
    "node": ">=6"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "scripts": {
    "start": "node server.js",
    "precommit": "lint-staged && npm test",
    "build": "cross-env NODE_ENV=production npm run clean && webpack --colors --profile --config webpack.config.prod.js",
    "start:prod": "cross-env NODE_ENV=production NODE_PORT=8081 node server.js",
    "start:dev": "node server.js",
    "start:ngrok": "cross-env NODE_URL=https://hablawebapp.ngrok.io HABLAAPI_BASE_URI=https://https://hablaapi.ngrok.io node server.js",
    "prestart": "npm run build",
    "lint": "eslint src",
    "stylelint": "stylelint src/**/*.css",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "clean": "shx rm -rf dist",
    "pristine": "npm run clean && shx rm -rf node_modules"
  },
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jsdom-quokka-plugin": "^1.0.10",
    "lint-staged": "^4.3.0"
  },
  "dependencies": {
    "antd": "^3.4.1",
    "autoprefixer": "^7.2.6",
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.4.1",
    "countries-and-timezones": "^1.0.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.10",
    "d3": "^4.13.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "helmet": "^3.11.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "i18n-js": "^3.0.3",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "nodemon": "^1.17.3",
    "postcss-loader": "^2.1.0",
    "re-reselect": "^0.6.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-file-drop": "^0.1.9",
    "react-fontawesome": "^1.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.10.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "shx": "^0.2.2",
    "socket.io-client": "^2.1.0",
    "socketio-wildcard": "^2.0.0",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-recommended": "^1.0.0",
    "validate.js": "^0.11.1",
    "victory": "^0.24.5",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.11.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint",
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./webpack.config.js"
        },
        "node": {
          "paths": [
            "./src"
          ]
        }
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "react/jsx-filename-extension": "off",
      "react/forbid-prop-types": "off",
      "react/no-array-index-key": "off",
      "react/no-danger": "off",
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/label-has-for": "off",
      "import/no-extraneous-dependencies": "off",
      "react/jsx-indent": [
        1,
        2
      ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-tabs": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-underscore-dangle": 0,
      "max-len": [
        1,
        180,
        4
      ],
      "arrow-body-style": [
        0
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-recommended"
  }
}
