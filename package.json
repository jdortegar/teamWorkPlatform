{
  "name": "webapp",
  "description": "Habla AI App",
  "version": "1.4.0",
  "private": true,
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Habla-Inc/habla-web.git"
  },
  "bugs": {
    "url": "https://github.com/Habla-Inc/habla-web/issues"
  },
  "homepage": "https://habla.ai",
  "author": "Josias Schneider <josias.schneider@habla.ai>, David Ortega <david.ortega@habla.ai>, Tho Truong <tho.truong@habla.ai>, Anthony Daga <anthony.daga@habla.ai>, Mike Somlo <mike.somlo@habla.ai>, Gonzalo Marquez <gonzalo@techlabs.com>",
  "license": "HABLA",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "start:prod": "cross-env NODE_ENV=production NODE_PORT=8081 node server.js",
    "build": "cross-env NODE_ENV=development npm run clean && webpack --colors --profile --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production npm run clean && webpack --colors --config webpack.config.prod.js",
    "prestart:prod": "npm run build:prod",
    "analyze": "cross-env NODE_ENV=production npm run clean && webpack --profile --config webpack.config.prod.js --json > webpack-stats.json",
    "lint": "eslint --fix src",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|ts|tsx|md)\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "validate": "npm run lint && npm run format && npm run test",
    "clean": "shx rm -rf dist",
    "pristine": "npm run clean && shx rm -rf node_modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.4",
    "jest-runner-eslint": "^0.7.3",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^7.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
